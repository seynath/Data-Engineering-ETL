{
  "data_asset_type": "Dataset",
  "expectation_suite_name": "silver_patients_suite",
  "expectations": [
    {
      "expectation_type": "expect_table_columns_to_match_ordered_list",
      "kwargs": {
        "column_list": [
          "patient_id",
          "first_name",
          "last_name",
          "dob",
          "age",
          "gender",
          "ethnicity",
          "insurance_type",
          "marital_status",
          "address",
          "city",
          "state",
          "zip",
          "phone",
          "email",
          "registration_date",
          "load_timestamp",
          "source_file",
          "record_hash"
        ]
      },
      "meta": {
        "notes": "Schema validation - column names and order"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "patient_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "patient_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "patient_id",
        "regex": "^PAT\\d{6}$"
      },
      "meta": {
        "notes": "ID pattern matching - PAT* format"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "age",
        "min_value": 0,
        "max_value": 120
      },
      "meta": {
        "notes": "Value range check for age"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "first_name"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "last_name"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "dob"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "load_timestamp"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "source_file"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "record_hash"
      }
    }
  ],
  "ge_cloud_id": null,
