{
  "data_asset_type": "Dataset",
  "expectation_suite_name": "silver_procedures_suite",
  "expectations": [
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "encounter_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_match_regex",
      "kwargs": {
        "column": "encounter_id",
        "regex": "^ENC\\d{6}$"
      },
      "meta": {
        "notes": "Foreign key ID pattern validation"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "procedure_code"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "procedure_cost",
        "min_value": 0,
        "max_value": null,
        "mostly": 0.95
      },
      "meta": {
        "notes": "Value range check - costs must be non-negative"
      }
    }
  ],
  "ge_cloud_id": null,
  "meta": {
    "great_expectations_version": "0.18.8"
  }
}
